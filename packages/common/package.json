{
  "name": "@booster-ai/common",
  "version": "3.2.0",
  "description": "Contains Booster common helpers used by the core and provider packages",
  "keywords": [
    "framework-common-helpers"
  ],
  "author": "Boosterin Labs SLU",
  "homepage": "https://boosterframework.com",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boostercloud/booster.git"
  },
  "scripts": {
    "format": "prettier --write --ext '.js,.ts' **/*.ts **/*/*.ts",
    "lint:check": "eslint --ext '.js,.ts' **/*.ts",
    "lint:fix": "eslint --quiet --fix --ext '.js,.ts' **/*.ts",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "test:helpers": "npm run test",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test:vitest": "vitest",
    "prepack": "tsc -b tsconfig.json"
  },
  "bugs": {
    "url": "https://github.com/boostercloud/booster/issues"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0 || >=22.0.0 <23.0.0"
  },
  "dependencies": {
    "tslib": "^2.4.0",
    "class-transformer": "~0.5.1",
    "execa": "^9.6.0",
    "effect": "3.16.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@booster-ai/eslint-config": "workspace:^3.2.0",
    "@booster-ai/metadata": "workspace:^3.2.0",
    "@types/chai": "5.2.2",
    "@types/chai-as-promised": "8.0.2",
    "@types/mocha": "10.0.10",
    "@types/node": "^22.15.21",
    "@types/rewire": "^2.5.28",
    "@types/sinon": "10.0.0",
    "@types/sinon-chai": "4.0.0",
    "@types/uuid": "10.0.0",
    "chai": "5.2.0",
    "chai-as-promised": "8.0.1",
    "mocha": "11.6.0",
    "nyc": "^17.1.0",
    "rewire": "8.0.0",
    "rimraf": "^6.0.1",
    "sinon": "9.2.3",
    "sinon-chai": "4.0.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.8.1",
    "typescript": "5.8.3",
    "vitest": "^2.1.8",
    "faker": "5.1.0",
    "@types/faker": "5.1.5",
    "fast-check": "^4.1.1",
    "graphql": "^16.6.0"
  }
}
