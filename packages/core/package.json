{
  "name": "@booster-ai/core",
  "version": "0.0.1",
  "description": "Library for your Booster apps",
  "author": "Boosterin Labs SLU",
  "homepage": "https://boosterframework.com",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boostercloud/booster.git"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "format": "prettier --write --ext '.js,.ts' **/*.ts **/*/*.ts",
    "lint:check": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --quiet --fix \"**/*.ts\"",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "prepack": "tsc -b tsconfig.json",
    "test:core": "npm run test",
    "test": "tsc --noEmit && cross-env BOOSTER_ENV=test nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\" 2>&1"
  },
  "bugs": {
    "url": "https://github.com/boostercloud/booster/issues"
  },
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "dependencies": {
    "@booster-ai/common": "workspace:^0.0.1",
    "@booster-ai/metadata": "workspace:^0.0.1",
    "fp-ts": "2.11.0",
    "graphql-scalars": "1.24.1",
    "graphql-subscriptions": "3.0.0",
    "inflected": "2.1.0",
    "iterall": "1.3.0",
    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "3.2.0",
    "tslib": "2.8.1",
    "effect": "3.16.11",
    "validator": "13.15.15",
    "@effect/platform-node": "0.86.5",
    "@effect/platform": "^0.85.2",
    "@effect/cluster": "0.39.5",
    "@effect/workflow": "0.2.4",
    "@effect/rpc": "0.62.4",
    "@effect/sql": "0.38.2",
    "@effect/experimental": "0.49.2",
    "fast-check": "4.1.1",
    "@effect/cli": "~0.66.4",
    "@effect/printer": "~0.44.11",
    "@effect/printer-ansi": "~0.44.11",
    "@effect/typeclass": "~0.35.11"
  },
  "devDependencies": {
    "@booster-ai/eslint-config": "workspace:^0.0.1",
    "@types/chai": "5.2.2",
    "@types/chai-as-promised": "8.0.2",
    "@types/inflected": "2.1.3",
    "@types/jsonwebtoken": "9.0.10",
    "@types/mocha": "10.0.10",
    "@types/node": "22.15.21",
    "@types/sinon": "17.0.4",
    "@types/sinon-chai": "4.0.0",
    "@types/validator": "13.15.2",
    "chai": "5.2.0",
    "chai-as-promised": "8.0.1",
    "cross-env": "7.0.3",
    "@faker-js/faker": "7.6.0",
    "mocha": "11.7.1",
    "mock-jwks": "3.3.5",
    "nyc": "17.1.0",
    "rimraf": "6.0.1",
    "sinon": "21.0.0",
    "sinon-chai": "4.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.8.3",
    "graphql": "16.6.0",
    "ts-patch": "3.3.0"
  }
}
