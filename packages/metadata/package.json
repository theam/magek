{
  "name": "@magek/metadata",
  "version": "0.0.1",
  "description": "Emits detailed metadata of your types. You can then get it in runtime to deal with schema-aware operation, like defining GraphQL schemas, ORM operations, etc.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theam/magek.git"
  },
  "keywords": [
    "metadata",
    "transformer",
    "magek",
    "reflect"
  ],
  "author": "Boosterin Labs SLU",
  "type": "module",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/theam/magek/issues"
  },
  "homepage": "https://magek.ai",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "dependencies": {
    "reflect-metadata": "0.2.2",
    "ts-morph": "26.0.0",
    "tslib": "2.4.0",
    "@effect-ts/core": "0.60.4"
  },
  "devDependencies": {
    "@magek/eslint-config": "workspace:^0.0.1",
    "@types/node": "22.18.0",
    "ts-node": "10.9.1",
    "ts-patch": "3.3.0",
    "typescript": "5.8.3",
    "@types/chai-as-promised": "8.0.2",
    "sinon": "21.0.0",
    "rimraf": "6.0.1",
    "chai-as-promised": "8.0.1"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint:check": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --quiet --fix \"**/*.ts\"",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "prepack": "tsc -b tsconfig.json",
    "test": "tsc -p tsconfig.test.json"
  }
}
