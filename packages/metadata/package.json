{
  "name": "@booster-ai/metadata",
  "version": "3.2.0",
  "description": "Emits detailed metadata of your types. You can then get it in runtime to deal with schema-aware operation, like defining GraphQL schemas, ORM operations, etc.",
  "repository": "https://github.com/boostercloud/booster.git",
  "keywords": [
    "metadata",
    "transformer",
    "booster",
    "reflect"
  ],
  "author": "Boosterin Labs SLU",
  "type": "module",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/boostercloud/booster/issues"
  },
  "homepage": "https://github.com/boostercloud/booster#readme",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0 <21.0.0 || >=22.0.0 <23.0.0"
  },
  "dependencies": {
    "reflect-metadata": "0.2.2",
    "ts-morph": "^26.0.0",
    "tslib": "^2.4.0",
    "@effect-ts/core": "^0.60.4"
  },
  "devDependencies": {
    "@booster-ai/eslint-config": "workspace:^3.2.0",
    "@types/node": "^22.15.21",
    "ts-node": "^10.9.1",
    "ts-patch": "3.3.0",
    "typescript": "5.8.3",
    "sinon": "9.2.3",
    "rimraf": "^6.0.1",
    "chai-as-promised": "8.0.1",
    "@types/chai-as-promised": "8.0.2"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint:check": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --quiet --fix \"**/*.ts\"",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "prepack": "tsc -b tsconfig.json",
    "test": "tsc -p tsconfig.test.json"
  }
}
