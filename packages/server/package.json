{
  "name": "@booster-ai/server",
  "version": "3.2.0",
  "type": "module",
  "description": "Debug your Booster projects locally",
  "keywords": [
    "server"
  ],
  "author": "Boosterin Labs SLU",
  "homepage": "https://boosterframework.com",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boostercloud/booster.git"
  },
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "dependencies": {
    "@booster-ai/common": "workspace:^3.2.0",
    "@seald-io/nedb": "4.1.1",
    "tslib": "2.4.0",
    "@effect-ts/core": "0.60.4",
    "fastify": "5.3.3"
  },
  "scripts": {
    "format": "prettier --write --ext '.js,.ts' **/*.ts **/*/*.ts",
    "lint:check": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --quiet --fix \"**/*.ts\"",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "prepack": "tsc -b tsconfig.json",
    "test:provider-local": "npm run test",
    "test": "BOOSTER_ENV=test nyc --extension .ts mocha --loader ts-node/esm --forbid-only \"test/**/*.test.ts\""
  },
  "bugs": {
    "url": "https://github.com/boostercloud/booster/issues"
  },
  "devDependencies": {
    "@booster-ai/eslint-config": "workspace:^3.2.0",
    "@types/chai": "5.2.2",
    "@types/chai-as-promised": "8.0.2",
    "@types/mocha": "10.0.10",
    "@types/node": "22.15.21",
    "@types/sinon": "17.0.4",
    "@types/sinon-chai": "4.0.0",
    "chai": "5.2.0",
    "chai-as-promised": "8.0.1",
    "@faker-js/faker": "7.6.0",
    "mocha": "11.7.1",
    "nyc": "17.1.0",
    "rimraf": "6.0.1",
    "sinon": "21.0.0",
    "sinon-chai": "4.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.8.3"
  },
  "ts-node": {
    "esm": true
  }
}
