{
  "name": "@booster-ai/server",
  "version": "3.2.0",
  "description": "Debug your Booster projects locally",
  "keywords": [
    "server"
  ],
  "author": "Boosterin Labs SLU",
  "homepage": "https://boosterframework.com",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boostercloud/booster.git"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0 || >=22.0.0 <23.0.0"
  },
  "dependencies": {
    "@booster-ai/common": "workspace:^3.2.0",
    "@seald-io/nedb": "4.1.1",
    "tslib": "^2.4.0",
    "@effect-ts/core": "^0.60.4",
    "fastify": "^5.3.3"
  },
  "scripts": {
    "format": "prettier --write --ext '.js,.ts' **/*.ts **/*/*.ts",
    "lint:check": "eslint --ext '.js,.ts' **/*.ts",
    "lint:fix": "eslint --quiet --fix --ext '.js,.ts' **/*.ts",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "prepack": "tsc -b tsconfig.json",
    "test:provider-local": "npm run test",
    "test": "BOOSTER_ENV=test nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test:vitest": "vitest"
  },
  "bugs": {
    "url": "https://github.com/boostercloud/booster/issues"
  },
  "devDependencies": {
    "@booster-ai/eslint-config": "workspace:^3.2.0",
    "@types/chai": "5.2.2",
    "@types/chai-as-promised": "8.0.2",
    "@types/faker": "5.1.5",
    "@types/mocha": "10.0.10",
    "@types/node": "^22.15.21",
    "@types/sinon": "10.0.0",
    "@types/sinon-chai": "4.0.0",
    "chai": "5.2.0",
    "chai-as-promised": "8.0.1",
    "faker": "5.1.0",
    "mocha": "11.6.0",
    "nyc": "^17.1.0",
    "rimraf": "^6.0.1",
    "sinon": "9.2.3",
    "sinon-chai": "4.0.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.8.1",
    "typescript": "5.8.3",
    "vitest": "^2.1.8"
  }
}
