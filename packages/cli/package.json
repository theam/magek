{
  "name": "@booster-ai/cli",
  "description": "CLI of the Booster Framework, the next level of abstraction for cloud-native applications",
  "version": "3.2.0",
  "type": "module",
  "author": "Boosterin Labs SLU",
  "homepage": "https://boosterframework.com",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "boost": "./bin/run"
  },
  "bugs": "https://github.com/boostercloud/booster/issues",
  "dependencies": {
    "@booster-ai/core": "workspace:^3.2.0",
    "@booster-ai/common": "workspace:^3.2.0",
    "@oclif/core": "4.4.0",
    "@oclif/plugin-help": "6.2.25",
    "chalk": "5.4.1",
    "execa": "9.6.0",
    "fp-ts": "2.11.0",
    "fs-extra": "11.3.0",
    "inquirer": "12.6.3",
    "mustache": "4.2.0",
    "ora": "8.2.0",
    "ts-morph": "26.0.0",
    "tslib": "2.4.0",
    "effect": "3.16.11",
    "plur": "4.0.0"
  },
  "devDependencies": {
    "@booster-ai/eslint-config": "workspace:^3.2.0",
    "@oclif/test": "4.1.10",
    "@types/chai": "5.2.2",
    "@types/chai-as-promised": "8.0.2",
    "@types/fs-extra": "11.0.4",
    "@types/mocha": "10.0.10",
    "@types/mustache": "4.2.6",
    "@types/node": "22.15.21",
    "@types/rewire": "2.5.28",
    "@types/sinon": "17.0.4",
    "@types/sinon-chai": "4.0.0",
    "chai": "5.2.0",
    "chai-as-promised": "8.0.1",
    "copyfiles": "2.3.0",
    "@faker-js/faker": "7.6.0",
    "fancy-test": "3.0.1",
    "mocha": "11.7.1",
    "nyc": "17.1.0",
    "rewire": "8.0.0",
    "rimraf": "6.0.1",
    "sinon": "21.0.0",
    "sinon-chai": "4.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.8.3",
    "ts-patch": "3.3.0"
  },
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "boost",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "command_not_found": "./dist/hooks/command_not_found/custom-parse"
    }
  },
  "repository": "boostercloud/booster",
  "scripts": {
    "format": "prettier --write --ext '.js,.ts' **/*.ts **/*/*.ts",
    "lint:check": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --quiet --fix \"**/*.ts\"",
    "build": "tsc -b tsconfig.json && copyfiles -f src/templates/*.stub dist/templates",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "prepack": "tsc -b tsconfig.json",
    "test:cli": "npm run test",
    "test": "nyc --extension .ts mocha --loader ts-node/esm --forbid-only  \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts",
  "ts-node": {
    "esm": true
  }
}
