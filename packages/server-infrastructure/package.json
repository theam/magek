{
  "name": "@magek/server-infrastructure",
  "version": "0.0.1",
  "description": "Compatibility wrapper for @magek/server/infrastructure",
  "keywords": [
    "server-infrastructure"
  ],
  "author": "Boosterin Labs SLU",
  "homepage": "https://magek.ai",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theam/magek.git"
  },
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "dependencies": {
    "@magek/server": "workspace:^0.0.1",
    "tslib": "2.8.1"
  },
  "scripts": {
    "format": "prettier --write --ext '.js,.ts' **/*.ts **/*/*.ts",
    "lint:check": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --quiet --fix \"**/*.ts\"",
    "build": "tsc -b tsconfig.src.json",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "prepack": "tsc -b tsconfig.src.json",
    "test:provider-local-infrastructure": "npm run test",
    "test": "echo 'Tests for @magek/server-infrastructure have been moved to @magek/server/test/infrastructure'"
  },
  "bugs": {
    "url": "https://github.com/theam/magek/issues"
  },
  "devDependencies": {
    "@magek/eslint-config": "workspace:^0.0.1",
    "@types/chai": "5.2.3",
    "@types/chai-as-promised": "8.0.2",
    "@types/mocha": "10.0.10",
    "@types/node": "22.19.3",
    "@types/node-schedule": "2.1.8",
    "@types/sinon": "21.0.0",
    "@types/sinon-chai": "4.0.0",
    "chai": "6.2.1",
    "chai-as-promised": "8.0.2",
    "@faker-js/faker": "10.1.0",
    "fastify": "5.6.2",
    "mocha": "11.7.5",
    "c8": "^10.1.3",
    "rimraf": "6.1.2",
    "sinon": "21.0.0",
    "sinon-chai": "4.0.1",
    "tsx": "^4.19.2",
    "typescript": "5.9.3"
  }
}
