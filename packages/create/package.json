{
  "name": "create-magek",
  "version": "0.0.7",
  "description": "Create Magek AI applications with one command",
  "type": "module",
  "main": "dist/cli.cjs",
  "bin": {
    "create-magek": "dist/cli.cjs"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "scripts": {
    "format": "prettier --write --ext '.js,.ts' **/*.ts **/*/*.ts",
    "lint:check": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --quiet --fix \"**/*.ts\"",
    "build": "tsup src/cli.ts --format cjs --target node22 --outDir dist --clean",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "prepack": "npm run build",
    "test:create": "npm run test",
    "test": "mocha test/**/*.test.ts"
  },
  "keywords": [
    "magek",
    "scaffold",
    "generator",
    "cli"
  ],
  "author": "Boosterin Labs SLU",
  "license": "Apache-2.0",
  "homepage": "https://magek.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theam/magek.git"
  },
  "bugs": {
    "url": "https://github.com/theam/magek/issues"
  },
  "dependencies": {
    "degit": "2.8.4",
    "prompts": "2.4.2",
    "kleur": "4.1.5",
    "globby": "16.1.0"
  },
  "devDependencies": {
    "@magek/eslint-config": "workspace:^0.0.7",
    "@types/node": "22.19.7",
    "@types/prompts": "2.4.9",
    "@types/chai": "5.2.3",
    "@types/mocha": "10.0.10",
    "@types/sinon": "21.0.0",
    "chai": "6.2.2",
    "mocha": "11.7.5",
    "sinon": "21.0.1",
    "tsx": "^4.19.2",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "@types/degit": "~2.8.6",
    "rimraf": "6.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
