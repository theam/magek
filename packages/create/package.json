{
  "name": "create-booster-ai",
  "version": "0.0.0",
  "description": "Create Booster AI applications with one command",
  "main": "dist/cli.js",
  "bin": {
    "create-booster-ai": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "template/**/*"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsup src/cli.ts --format cjs --target node22 --outDir dist --clean",
    "test": "mocha --recursive test/**/*.test.ts --require ts-node/register --timeout 10000",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "booster",
    "booster-ai",
    "scaffold",
    "generator",
    "cli"
  ],
  "author": "Booster Framework Team",
  "license": "Apache-2.0",
  "dependencies": {
    "degit": "^2.8.4",
    "prompts": "^2.4.2",
    "kleur": "^4.1.5",
    "globby": "^14.0.2"
  },
  "devDependencies": {
    "@booster-ai/eslint-config": "workspace:^3.2.0",
    "@types/node": "^22.15.21",
    "@types/prompts": "^2.4.9",
    "@types/chai": "5.2.2",
    "@types/mocha": "10.0.10",
    "chai": "5.2.0",
    "eslint": "^8.23.1",
    "mocha": "10.2.0",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.0",
    "typescript": "5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}