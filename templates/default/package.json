{
  "name": "{{PROJECT_NAME}}",
  "description": "{{description}}",
  "version": "{{version}}",
  "author": "{{author}}",
  "dependencies": {
    "tslib": "2.4.0",
    "@magek/core": "{{magekVersion}}",
    "@magek/common": "{{magekVersion}}",
    "@magek/cli": "{{magekVersion}}",
    "@magek/adapter-event-store-nedb": "{{magekVersion}}",
    "{{providerPackageName}}": "{{magekVersion}}"
  },
  "devDependencies": {
