    "{{providerPackageName}}-infrastructure": "{{magekVersion}}",
    "rimraf": "5.0.0",
    "@magek/eslint-config": "${VERSION}",
    "mocha": "11.7.1",
    "@types/mocha": "10.0.1",
    "@types/jsonwebtoken": "9.0.1",
    "nyc": "15.1.0",
    "typescript": "5.1.6",
