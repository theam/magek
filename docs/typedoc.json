{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "../packages/core/src/index.ts",
    "../packages/common/src/index.ts",
    "../packages/server/src/index.ts",
    "../packages/cli/src/index.ts"
  ],
  "entryPointStrategy": "expand",
  "name": "Magek Framework",
  "readme": "none",
  "projectDocuments": [
    "./content/index.md"
  ],
  "sortEntryPoints": false,
  "tsconfig": "./tsconfig.typedoc.json",
  "exclude": [
    "../e2e/**/*",
    "../templates/**/*",
    "../adapters/**/*",
    "../packages/create/**/*",
    "../packages/framework-provider-local/**/*",
    "**/test/**/*",
    "**/tests/**/*",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "excludeInternal": true,
  "hideGenerator": false,
  "includeVersion": true,
  "navigationLinks": {
    "GitHub": "https://github.com/theam/magek"
  },
  "navigation": {
    "includeCategories": false,
    "includeGroups": true,
    "includeFolders": false
  },
  "customCss": "./custom.css",
  "customJs": "./version-selector.js",
  "plugin": [
    "typedoc-plugin-merge-modules",
    "./custom-theme.mjs"
  ],
  "theme": "magek",
  "mergeModulesMergeMode": "project",
  "categorizeByGroup": true,
  "groupOrder": [
    "Classes",
    "Interfaces",
    "Type Aliases",
    "Functions",
    "Variables",
    "*"
  ],
  "sort": ["source-order"],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method",
    "Reference"
  ],
  "highlightLanguages": [
    "bash",
    "console",
    "css",
    "graphql",
    "html",
    "http",
    "javascript",
    "json",
    "jsonc",
    "json5",
    "shell",
    "tsx",
    "typescript",
    "yaml"
  ]
}
